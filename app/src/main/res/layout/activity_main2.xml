<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/activity_main2"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    tools:context="com.example.administrator.appweb.Main2Activity">

    <Button
        android:text="美团"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentTop="true"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_marginTop="17dp"
        android:id="@+id/meituan" />

    <Button
        android:text="淘宝"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/meituan"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:id="@+id/taobao" />

    <Button
        android:text="百度网盘"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignTop="@+id/meituan"
        android:layout_toRightOf="@+id/meituan"
        android:layout_toEndOf="@+id/meituan"
        android:layout_marginLeft="24dp"
        android:layout_marginStart="24dp"
        android:id="@+id/baiduwangpan" />

    <Button
        android:text="携程"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignTop="@+id/baiduwangpan"
        android:layout_alignParentRight="true"
        android:layout_alignParentEnd="true"
        android:layout_marginRight="25dp"
        android:layout_marginEnd="25dp"
        android:id="@+id/xiecheng" />

    <Button
        android:text="大众"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/xiecheng"
        android:layout_alignLeft="@+id/xiecheng"
        android:layout_alignStart="@+id/xiecheng"
        android:id="@+id/dazhong" />

    <Button
        android:text="百度地图"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/baiduwangpan"
        android:layout_alignLeft="@+id/baiduwangpan"
        android:layout_alignStart="@+id/baiduwangpan"
        android:id="@+id/baiduditu" />

    <Button
        android:text="QQ邮箱"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/taobao"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_marginTop="13dp"
        android:id="@+id/qqyouxiang" />

    <Button
        android:text="哔哩哔哩"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/bilibili"
        android:layout_alignBaseline="@+id/qqyouxiang"
        android:layout_alignBottom="@+id/qqyouxiang"
        android:layout_alignLeft="@+id/dazhong"
        android:layout_alignStart="@+id/dazhong" />

    <Button
        android:text="知乎"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignTop="@+id/qqyouxiang"
        android:layout_alignLeft="@+id/baiduditu"
        android:layout_alignStart="@+id/baiduditu"
        android:id="@+id/zhihu" />

    <Button
        android:text="知乎日报"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="15dp"
        android:id="@+id/zhihuribao"
        android:layout_below="@+id/qqyouxiang"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true" />


</RelativeLayout>
